{
  "viewAll": {
    "command": "`$kanban [-A]`",
    "desc": "display the entire board"
  },
  "backlog": {
    "command": "`$kanban -B`",
    "desc": "display the Backlog tasks"
  },
  "inprogress": {
    "command": "`$kanban -I`",
    "desc": "display the In-Progress tasks"
  },
  "completed": {
    "command": "`$kanban -C`",
    "desc": "display the Completed tasks"
  },
  "add": {
    "command": "`$kanban add <Task>`",
    "desc": "default, adding a task to bottom row of \"Backlog\""
  },
  "remove": {
    "command": "`$kanban remove <Task>`",
    "desc": "remove selected tasks"
  },
  "edit": {
    "command": "`$kanban edit <Task> <New Task>`",
    "desc": "replace selected task with a new task"
  },
  "forward": {
    "command": "`$kanban forward <Task>`",
    "desc": "shift selected task forward from \"Backlog\" to \"In-Progress\""
  },
  "complete": {
    "command": "`$kanban complete <Task>`",
    "desc": "shift selected task from \"In-Progress\" to \"Completed\""
}
